datasource db {
    // could be postgresql or mysql
    provider = "mysql"
    url      = env("DATABASE_URL") //"file:dev.db"
}

generator db {
    provider = "go run github.com/steebchen/prisma-client-go"
}

model Post {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    title     String
    body      String
}
